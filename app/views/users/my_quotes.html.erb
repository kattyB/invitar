<div class="dashbord">
  <div class="dashbord_tCrv">
    <!---->
  </div>
  <div class="dashbord_mRepeat">
    <div class="midshade">
      <ul class="language">
        <li>
          <span class="english">English</span>
        </li>
        <li>
          <a href="#" class="spanish">Espanol</a>
        </li>
      </ul>
      <ul>
        <li class="first">
          <div class="profileBox">


            <div style="background-image: url('/images/topcrv.png'); width: 924px; height: 10px; margin-top: 33px;"></div>

            <div class="tab_container">



              <div id="tab1" class="tab_content">
                <div style="margin-bottom: 8px;"><strong>Quotes</strong></div>

                <% @quotes.each do |q| %>
                  <%
                  event = Event.find(q.id_event)
                  count = 0
                  new = 0
                %>
                  <% solicitudes = QuoteSolicitations.where(:quote_id => q.id) %>

                  <% solicitudes.each do |s| %>
                    <div class="message">
                      <div style="margin-right: 60px; margin-left: 30px; margin-top: 8px;">
                        <div style="margin-bottom: 10px;"><label><strong><%= link_to "#{s.solicitude} for #{event.name} ", "quote_view?id=#{s.id}"%></strong></label></div>

                        <% csolicitudes = CompanySolicitude.where(:solicitude_id => s.id) %>

                        <% csolicitudes.each do |cs| %>
                          <%
                          messages = Message.where(:csolicitude_id => cs.id, :parent => 0)
                          count += messages.length
                          messages_new = Message.where(:csolicitude_id => cs.id,  :is_new => true)
                          new = messages_new.length;
                        %>
                        <% end %>
                        <div style="margin-bottom: 10px;">
                          <div style=" float: left; color: #7B7073; font-weight: bold; font-size: 14px; margin-right: 3px;"><label><%= count %> quotes received for this event</label></div>

                          <% if(new > 0) %>

                            <div style="font-size: 14px;"> (<%= new %> New Received )</div>
                          <% end %>
                          <div>
                            &emsp;
                          </div>
                        </div>
                        <div style="font-size: 14px;"><label style="font-weight: bold; color: #7B7073;">Quote Request on</label> <%= q.created_at.to_s.first(10)  %></div>
                      </div>
                      <div style="margin-top: 20px; font-size: 14px;"></div>
                    </div>



                  <% end %>

                <% end %>

              </div>

            </div>
          </div>

        </li>
        <li>
          <div class="separator"></div>
        </li>
      </ul>

    </div>

  </div>
  <div class="dashbord_bCrv"></div>
</div>

