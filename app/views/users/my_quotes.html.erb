  }

  #frame .dashbord .dashbord_mRepeat .midshade ul li  ul.tabs li a:hover
  {
    background: #ccc;
  }

  html ul.tabs li.active, html ul.tabs li.active a:hover
  { /*--Estate seguro de que a la tab activa no se le aplicarán estas propiedades hover--*/
    background: #fff;
    border-bottom: 1px solid #fff; /*--Esto hace que la tab activa esté conectada con respecto a su contenido--*/
  }

  .tab_container
  {

    float: none;
    /*border: 1px solid #999;
    border-top: none;*/
    overflow: hidden;
    clear: both;
    float: left; width: 100%;
    background-image: url('/images/midRep.png');
    width: 924px;

