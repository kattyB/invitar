<h1>Listing quotes</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Category</th>
    <th>Description</th>
    <th>Zip</th>
    <th>City</th>
    <th>Miles way</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @quotes.each do |quote| %>
  <tr>
    <td><%= quote.title %></td>
    <td><%= quote.category_id %></td>
    <td><%= quote.description %></td>
    <td><%= quote.zip %></td>
    <td><%= quote.city_id %></td>
    <td><%= quote.miles_way %></td>
    <td><%= quote.user_id %></td>
    <td><%= link_to 'Show', quote %></td>
    <td><%= link_to 'Edit', edit_quote_path(quote) %></td>
    <td><%= link_to 'Destroy', quote, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Quote', new_quote_path %>
